<link:linkbase 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xbrli="http://www.xbrl.org/2003/instance" 
  xmlns:xlink="http://www.w3.org/1999/xlink" 
  xmlns:link="http://www.xbrl.org/2003/linkbase" 
  xmlns:gen="http://xbrl.org/2008/generic" 
  xmlns:variable="http://xbrl.org/2008/variable"
  xmlns:tf="http://xbrl.org/2008/filter/tuple"
  xmlns:acf="http://xbrl.org/2010/filter/aspect-cover"  
  xmlns:find="http://www.eurofiling.info/xbrl/ext/filing-indicators"   
  xmlns:xfi="http://www.xbrl.org/2008/function/instance" 
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xsi:schemaLocation="http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd http://xbrl.org/2008/generic http://www.xbrl.org/2008/generic-link.xsd http://xbrl.org/2008/variable http://www.xbrl.org/2008/variable.xsd http://xbrl.org/2008/filter/tuple http://www.xbrl.org/2008/tuple-filter.xsd http://xbrl.org/2010/filter/aspect-cover http://xbrl.org/2010/aspect-cover-filter.xsd">
  
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:type="simple" xlink:href="http://www.xbrl.org/2008/variable.xsd#variable-filter" />
  
  <gen:link xlink:type="extended" xlink:role="http://www.xbrl.org/2003/role/link">
    <variable:factVariable xlink:type="resource" xlink:label="pivot-var" bindAsSequence="true" matches="true" id="pivot-var" />
    <tf:parentFilter xlink:type="resource" xlink:label="find-tuple-filter" id="find-tuple-filter">
      <tf:parent>
        <tf:qname>find:fIndicators</tf:qname>        
      </tf:parent>
    </tf:parentFilter>
    <acf:aspectCover xlink:type="resource" xlink:label="all-cover-filter">
      <acf:aspect>all</acf:aspect>
    </acf:aspectCover>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" xlink:from="pivot-var" xlink:to="find-tuple-filter" cover="true" complement="false" order="1"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" xlink:from="pivot-var" xlink:to="all-cover-filter" cover="true" complement="false" order="2"/>    
  </gen:link>
</link:linkbase>